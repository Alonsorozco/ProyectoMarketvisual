 <% @user_publications.each do |publication| %>
    <tr>
        <td><%= publication.title%></td>
        <td><%= publication.description%></td>
        <td><%= publication.value%></td>
        <td><%= link_to 'Show', publication %></td>
        <td><%= link_to 'Edit', edit_quote_path(publication) %></td>
        <td><%= link_to 'Destroy', publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
<% end %>